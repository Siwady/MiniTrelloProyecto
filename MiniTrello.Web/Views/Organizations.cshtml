<div class="container" ng-repeat="organization in organizations">
    <div class="row">
        <h2>{{organization.Title}}</h2>
        <ul class="ds-btn nav navbar-nav navbar-left" ng-repeat="board in organization.boards">
            <li style="padding-left: 10px;">
                <a class="btn btn-lg btn-success " href="http://dotstrap.com/">
                    <i class="glyphicon glyphicon-dashboard pull-left"></i><span>{{board.Title}}<br><small>{{board.Administrator}}</small></span>
                </a>
            </li>
        </ul>
    </div>
</div>